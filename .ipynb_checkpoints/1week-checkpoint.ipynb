    "src = cv2.imread('image/book.jpg')\n",
    "src = cv2.resize(src,(0,0), fx = 1, fy = 1 , interpolation = cv2.INTER_AREA)\n",
    "\n",
    "height, width, channel = src.shape\n",
    "\n",
    "srcPoint = np.array([[90, 53], [504, 26], [48, 687], [637, 650]], dtype = np.float32)\n",
    "dstPoint = np.array([[0,0],[width,0],[0,height],[width,height]], dtype = np.float32)\n",
    "\n",
    "matrix = cv2.getPerspectiveTransform(srcPoint, dstPoint)\n",
    "dst = cv2.warpPerspective(src,matrix, (width, height))\n",
