   "source": [
    "## 빨간색은 병합\n",
    "\n",
    "src = cv2.imread('image/tomato.jpg',1)\n",
    "src = cv2.resize(src, dsize=(640, 480),interpolation = cv2.INTER_AREA)\n",
    "hsv = cv2.cvtColor(src,cv2.COLOR_BGR2HSV)\n",
    "h, s, v = cv2.split(hsv)\n",
    "\n",
    "lower_red = cv2.inRange(hsv, (0,30,30), (5,255,255))\n",
    "upper_red = cv2.inRange(hsv, (170,30,30), (179,255,255))\n",
    "added_red = cv2.addWeighted(lower_red,1.0,upper_red,1.0,0.0)\n",
    "\n",
    "what_color = cv2.bitwise_and(hsv, hsv, mask = added_red)\n",
    "what_color = cv2.cvtColor(what_color, cv2.COLOR_HSV2BGR)\n",
    "\n",
    "\n",
    "cv2.imshow('lower',lower_red)\n",
    "cv2.imshow('upper',upper_red)\n",
    "cv2.imshow('added',added_red)\n",
    "cv2.imshow(\"what_color\", what_color)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  },
