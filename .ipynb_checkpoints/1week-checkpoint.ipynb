    "## 화전행렬함수 만들어서 회전시키기, 테두리외삽법 연습\n",
    "\n",
    "src = cv2.imread('image/aaa.jpg', cv2.IMREAD_COLOR)\n",
    "src = cv2.resize(src, dsize=(0,0), fx = 0.5, fy = 0.5, interpolation = cv2.INTER_AREA)\n",
    "height, width, channel = src.shape\n",
    "\n",
    "\n",
    "matrix = cv2.getRotationMatrix2D((width/2,height/2), 270, 1) ## 행렬의 형태는 2x3\n",
    "dst = cv2.warpAffine(src, matrix, (width, height), borderMode = cv2.BORDER_CONSTANT, borderValue = (0,0,255))\n",
    "##width,height 대신 0,0도 가능.\n",
    "\n",
    "\n",
    "cv2.imshow('src', src)\n",
    "cv2.imshow('dst', dst)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
